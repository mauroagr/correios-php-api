{
  "name": "aguiarrdev/correios",
  "version": "1.0.0",
  "description": "Correios API library for PHP",
  "license": "GPL-3.0",
  "scripts": {
    "test": "./vendor/bin/pest"
  },
  "support": {
    "issues": "https://github.com/aguiarrdev/correios/issues",
    "source": "https://github.com/aguiarrdev/correios"
  },
  "keywords": [
    "library",
    "envio",
    "rastreio",
    "shipping",
    "correios"
  ],
  "authors": [
    {
      "name": "Matheus Aguiar",
      "email": "aguiartgv@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Correios\\": "src/"
    },
    "files": [
      "src/Helpers/Settings.php",
      "src/Helpers/Cep.php"
    ]
  },
  "require-dev": {
    "pestphp/pest": "^2.9",
    "pestphp/pest-plugin-faker": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
